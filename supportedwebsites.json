    "post_link_xpaths": "//div[@class='wapper-posts']/footer/div/a/@href",
    "download_link_xpaths": [["//a[@id='link-download']/@href"]]
  },
  "sub2fm": {
    "under_dev": true,
    "link": "https://subf2m.co",
    "search_link": "https://subf2m.co/subtitles/searchbytitle?query={q}",
