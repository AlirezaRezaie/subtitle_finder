    "post_link_xpaths": "//div[@class='cat-post-titel']/h2/a/@href",
    "download_link_xpaths": [
      ["//div[@class='new-link-3']/a/@href"],
      ["//ul[@class='download1']/a/@href"]
    ]
  },
  "subtitlestar": {
    "under_dev": false,
    "link": "https://subtitlestar.com",
